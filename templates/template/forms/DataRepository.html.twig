<form class="entityForm" entityType="DataRepository"
{% if is_granted('CAN_EDIT', DataRepository) %}
 editable
{% endif %}
_action="{{path('pelagos_api_data_repositories_get_collection')}}">

    <input type="hidden" name="id" value="{{DataRepository.getId}}">

