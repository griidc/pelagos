<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{% for dataset in datasets %}
    <url>
        <loc>{{ vanityurl('pelagos_app_ui_dataland_default', {'udi': dataset.udi}) }}</loc>
        <lastmod>{{ (sitemapMinDate is not empty and sitemapMinDate > dataset.modificationTimeStamp) ? sitemapMinDate|date('Y-m-d') : dataset.modificationTimeStamp|date('Y-m-d') }}</lastmod>
    </url>
{% endfor %}
</urlset>
