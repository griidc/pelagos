{
    "name": "griidc/pelagos",
    "type": "project",
    "description": "System for the management, discovery, and dissemination of citable scientific datasets",
    "keywords": ["PHP", "Symfony 4", "Doctrine", "science data", "marine data", "GoMRI", "data distribution", "citation tracking", "data submission management"],
    "homepage": "https://github.com/griidc/pelagos/",
    "license": "BSD-2-Clause",
    "require": {
        "php": "^8.2",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "ext-filter": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-ldap": "*",
        "ext-openssl": "*",
        "ext-pdo_pgsql": "*",
        "ext-simplexml": "*",
        "ext-tidy": "*",
        "ext-xml": "*",
        "ext-xsl": "*",
        "lib-libxml": "*",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^3.3",
        "easycorp/easyadmin-bundle": "^4.12.0",
        "easyrdf/easyrdf": "^1.1",
        "fdekker/log-viewer-bundle": "^2.0",
        "friendsofsymfony/elastica-bundle": "^6.1.0",
        "friendsofsymfony/jsrouting-bundle": "^3.0",
        "friendsofsymfony/rest-bundle": "^3.0",
        "guzzlehttp/guzzle": "^7.6",
        "hidehalo/nanoid-php": "^2.0",
        "jms/serializer-bundle": "^5.0",
        "knplabs/knp-snappy-bundle": "^1.10",
        "maennchen/zipstream-php": "^3.0",
        "oneup/flysystem-bundle": "^4.0",
        "opensoft/doctrine-postgres-types": "^1.2",
        "phayes/geophp": "^1.2",
        "ramsey/uuid": "^4.1",
        "symfony/asset": "6.4.*",
        "symfony/console": "6.4.*",
        "symfony/doctrine-messenger": "6.4.*",
        "symfony/dotenv": "6.4.*",
        "symfony/flex": "^2.4",
        "symfony/form": "6.4.*",
        "symfony/framework-bundle": "6.4.*",
        "symfony/ldap": "6.4.*",
        "symfony/mailer": "6.4.*",
        "symfony/maker-bundle": "^1.13",
        "symfony/messenger": "6.4.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/runtime": "6.4.*",
        "symfony/security-bundle": "6.4.*",
        "symfony/templating": "6.4.*",
        "symfony/translation": "6.4.*",
        "symfony/twig-bundle": "6.4.*",
        "symfony/validator": "6.4.*",
        "symfony/webpack-encore-bundle": "^2.0",
        "symfony/yaml": "6.4.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.13",
        "mikey179/vfsstream": "^1.6",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^1.9",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-symfony": "^1.2",
        "psalm/plugin-symfony": "^5.0",
        "rector/rector": "^1.2",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/browser-kit": "6.4.*",
        "symfony/css-selector": "6.4.*",
        "symfony/debug-bundle": "6.4.*",
        "symfony/phpunit-bridge": "7.2.*",
        "symfony/stopwatch": "6.4.*",
        "symfony/web-profiler-bundle": "6.4.*",
        "vimeo/psalm": "^5.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "platform": {
            "php": "8.3"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "App\\Tests\\Helpers\\": "tests/helpers",
            "App\\Tests\\Security\\": "tests/unit/Security"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "fos:js-routing:dump --format=json --target=public/js/fos_js_routes.json": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.4.*"
        }
    }
}
